group {
  name: "amsn2/login.username_label";
  parts {
    part {
      name: "label";
      type: TEXT;
      effect: GLOW;
      scale: 1;
      description {
        state: "default" 0.0;
        color: 225 225 225 255;
        color2: 0 0 0 255;
        visible: 1;
        text {
          align: 0.0 0.5;
          font: "VeraBd";
          size: 10;
          text: "Username:";
          min: 0 1;
          elipsis: 0.0;
        }
      }
    }
  }
}
group {
  name: "amsn2/login.username";
  parts {
    part {
      name: "username";
      type: EXTERNAL;
      source: "elm/scrolled_entry";
      scale: 1;
      description {
        state: "default" 0.0;
        params {
           string: "text" "";
           bool: "editable" "1";
           bool: "single line" "1";
           bool: "password" "0";
        }
      }
    }
  }
}

